{
  "components": [
    {
      "filename": "trust_graph.wasm",
      "package_name": "trust-graph",
      "package_version": "0.9.2",
      "trigger": {
        "event": {
          "contract_json_path": "${networks[]}.contracts.merkleSnapshot",
          "event": "MerklerTrigger(uint64)"
        }
      },
      "submit": {
        "contract_json_path": "${networks[]}.contracts.merkleSnapshot"
      },
      "config": {
        "file": "trust_graph"
      },
      "env_variables": ["WAVS_ENV_PINATA_API_URL", "WAVS_ENV_PINATA_API_KEY"]
    },
    {
      "filename": "trust_graph.wasm",
      "package_name": "trust-graph",
      "package_version": "0.9.2",
      "trigger": {
        "event": {
          "contract_json_path": "wavs_indexer",
          "event": "EventIndexed(bytes32,address,string,address[],string[])"
        }
      },
      "submit": {
        "contract_json_path": "${networks[]}.contracts.merkleSnapshot"
      },
      "config": {
        "file": "trust_graph"
      },
      "env_variables": ["WAVS_ENV_PINATA_API_URL", "WAVS_ENV_PINATA_API_KEY"]
