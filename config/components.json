        "event": {
          "contract_json_path": "${networks[]}.contracts.merkleSnapshot",
          "event": "MerkleRootUpdated(bytes32,bytes32,string,uint256)"
        }
      },
      "submit": {
        "contract_json_path": "${networks[]}.contracts.safe.signerSyncManager"
      },
      "config": {
        "file": "safe_signer_sync"
      },
      "env_variables": ["WAVS_ENV_PINATA_API_URL", "WAVS_ENV_PINATA_API_KEY"]
    },
