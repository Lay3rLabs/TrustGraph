{
  "components": [
    {
      "filename": "geyser.wasm",
      "package_name": "geyser",
      "package_version": "0.1.0",
      "trigger_event": "UpdateService(string)",
      "trigger_json_path": "geyser.trigger",
      "submit_json_path": "geyser.trigger",
      "config_values": {
        "chain_name": "${CHAIN_NAME}"
      }
    },
    {
      "filename": "wavs_eas_attest.wasm",
      "package_name": "wavs-eas-attest",
      "package_version": "0.1.0",
      "trigger_event": "AttestationRequested(address,bytes32,address,bytes)",
      "trigger_json_path": "eas.contracts.attest_trigger",
      "submit_json_path": "eas.contracts.attester",
      "config_values": {
        "eas_address": "${EAS_ADDRESS}",
        "indexer_address": "${INDEXER_ADDRESS}",
        "chain_name": "${CHAIN_NAME}"
      }
    },
    {
      "filename": "wavs_eas_compute.wasm",
      "package_name": "wavs-eas-compute",
      "package_version": "0.1.0",
      "trigger_event": "Attested(address,address,bytes32,bytes32)",
      "trigger_json_path": "eas.contracts.indexer_resolver",
      "submit_json_path": "governance.voting_power",
      "config_values": {
        "eas_address": "${EAS_ADDRESS}",
        "indexer_address": "${INDEXER_ADDRESS}",
        "chain_name": "${CHAIN_NAME}"
      }
    },
    {
      "filename": "merkler.wasm",
      "package_name": "merkler",
      "package_version": "0.1.0",
      "trigger_event": "MerklerTrigger(uint64)",
      "trigger_json_path": "merkler.merkle_snapshot",
      "submit_json_path": "merkler.merkle_snapshot",
      "config_values": {
        "events_dir": "./events",
        "eas_address": "${EAS_ADDRESS}",
        "indexer_address": "${INDEXER_ADDRESS}",
        "chain_name": "${CHAIN_NAME}",
        "recognition_schema_uid": "${RECOGNITION_SCHEMA_UID}",
        "vouching_schema_uid": "${VOUCHING_SCHEMA_UID}",
        "pagerank_points_pool": "10000",
        "pagerank_trusted_seeds": "0x70997970C51812dc3A010C7d01b50e0d17dc79C8",
        "pagerank_trust_multiplier": "10.5",
        "pagerank_trust_boost": "0.99"
      },
      "env_variables": ["WAVS_ENV_PINATA_API_URL", "WAVS_ENV_PINATA_API_KEY"]
    },
    {
      "filename": "merkler.wasm",
      "package_name": "merkler",
      "package_version": "0.1.0",
      "trigger_event": "EventIndexed(bytes32,address,string,address[],string[])",
      "trigger_json_path": "wavs_indexer",
      "submit_json_path": "merkler.merkle_snapshot",
      "config_values": {
        "events_dir": "./events",
        "eas_address": "${EAS_ADDRESS}",
        "indexer_address": "${INDEXER_ADDRESS}",
        "chain_name": "${CHAIN_NAME}",
        "recognition_schema_uid": "${RECOGNITION_SCHEMA_UID}",
        "vouching_schema_uid": "${VOUCHING_SCHEMA_UID}",
        "pagerank_points_pool": "10000",
        "pagerank_trusted_seeds": "0x70997970C51812dc3A010C7d01b50e0d17dc79C8",
        "pagerank_trust_multiplier": "10.5",
        "pagerank_trust_boost": "0.99"
      },
      "env_variables": ["WAVS_ENV_PINATA_API_URL", "WAVS_ENV_PINATA_API_KEY"]
    },
    {
      "filename": "merkler_pruner.wasm",
      "package_name": "merkler-pruner",
      "package_version": "0.1.0",
      "trigger_block_interval": "100",
      "submit_json_path": "merkler.merkle_snapshot",
      "config_values": {},
      "env_variables": []
    },
    {
      "filename": "wavs_safe_signer_sync.wasm",
      "package_name": "wavs-safe-signer-sync",
      "package_version": "0.1.0",
      "trigger_event": "MerkleRootUpdated(bytes32,bytes32,string)",
      "trigger_json_path": "zodiac_safes.safe1.merkle_gov_module",
      "submit_json_path": "zodiac_safes.safe1.signer_module",
      "config_values": {
        "top_n_signers": "5",
        "min_threshold": "3",
        "signer_module_address": "${SIGNER_MANAGER_ZODIAC_MODULE}",
        "chain_name": "${CHAIN_NAME}"
      },
      "env_variables": ["WAVS_ENV_PINATA_API_URL", "WAVS_ENV_PINATA_API_KEY"]
    },
    {
      "filename": "dao_agent.wasm",
      "package_name": "dao-agent",
      "package_version": "0.1.0",
      "trigger_event": "NewTrigger(bytes)",
      "trigger_json_path": "eas.contracts.attest_trigger",
      "submit_json_path": "zodiac_safes.safe2.wavs_module",
      "config_values": {},
      "env_variables": ["WAVS_ENV_PINATA_API_URL", "WAVS_ENV_PINATA_API_KEY"]
    },
    {
      "filename": "prediction_market_oracle.wasm",
      "package_name": "predictionmarketoracle",
      "package_version": "0.1.0",
      "trigger_event": "NewTrigger(bytes)",
      "trigger_json_path": "prediction_market.oracle_controller",
      "submit_json_path": "prediction_market.oracle_controller",
      "config_values": {
        "market_maker": "${MARKET_MAKER_ADDRESS}",
        "conditional_tokens": "${CONDITIONAL_TOKENS_ADDRESS}",
        "chain_name": "${CHAIN_NAME}",
        "resolver_type": "price",
        "resolver_config": "coin_market_cap_id:1;threshold:1.0"
      }
    },
    {
      "filename": "llm_attester.wasm",
      "package_name": "llm-attester",
      "package_version": "0.1.0",
      "trigger_event": "Attested(address,address,bytes32,bytes32)",
      "trigger_json_path": "eas.contracts.payable_indexer_resolver",
      "submit_json_path": "eas.contracts.attester",
      "config_values": {
        "eas_address": "${EAS_ADDRESS}",
        "chain_name": "${CHAIN_NAME}",
        "llm_model": "llama3.2",
        "llm_temperature": "0.0",
        "llm_top_p": "1.0",
        "llm_seed": "42",
        "llm_max_tokens": "100",
        "llm_context_window": "250",
        "llm_system_message": "You are a blockchain god evaluating attestation statements to determine if you like them",
        "attestation_schema_uid": "${STATEMENT_SCHEMA_ID}",
        "submit_schema_uid": "${LIKE_SCHEMA_ID}",
        "attestation_revocable": "true",
        "attestation_expiration": "0",
        "attestation_value": "0"
      },
      "env_variables": []
    },
    {
      "filename": "wavs_indexer.wasm",
      "package_name": "wavs-indexer",
      "package_version": "0.1.0",
      "trigger_event": "AttestationAttested(address,bytes32)",
      "trigger_json_path": "eas.contracts.indexer_resolver",
      "submit_json_path": "wavs_indexer"
    },
    {
      "filename": "wavs_indexer.wasm",
      "package_name": "wavs-indexer",
      "package_version": "0.1.0",
      "trigger_event": "AttestationRevoked(address,bytes32)",
      "trigger_json_path": "eas.contracts.indexer_resolver",
      "submit_json_path": "wavs_indexer"
    },
    {
      "filename": "wavs_indexer.wasm",
      "package_name": "wavs-indexer",
      "package_version": "0.1.0",
      "trigger_event": "AttestationAttested(address,bytes32)",
      "trigger_json_path": "eas.contracts.payable_indexer_resolver",
      "submit_json_path": "wavs_indexer"
    },
    {
      "filename": "wavs_indexer.wasm",
      "package_name": "wavs-indexer",
      "package_version": "0.1.0",
      "trigger_event": "AttestationRevoked(address,bytes32)",
      "trigger_json_path": "eas.contracts.payable_indexer_resolver",
      "submit_json_path": "wavs_indexer"
    },
    {
      "filename": "wavs_indexer.wasm",
      "package_name": "wavs-indexer",
      "package_version": "0.1.0",
      "trigger_event": "AttestationAttested(address,bytes32)",
      "trigger_json_path": "eas.contracts.attester_indexer_resolver",
      "submit_json_path": "wavs_indexer"
    },
    {
      "filename": "wavs_indexer.wasm",
      "package_name": "wavs-indexer",
      "package_version": "0.1.0",
      "trigger_event": "AttestationRevoked(address,bytes32)",
      "trigger_json_path": "eas.contracts.attester_indexer_resolver",
      "submit_json_path": "wavs_indexer"
    },
    {
      "filename": "wavs_indexer.wasm",
      "package_name": "wavs-indexer",
      "package_version": "0.1.0",
      "trigger_event": "Interaction(address,string,string[],bytes)",
      "trigger_json_path": "prediction_market.conditional_tokens",
      "submit_json_path": "wavs_indexer"
    },
    {
      "filename": "wavs_indexer.wasm",
      "package_name": "wavs-indexer",
      "package_version": "0.1.0",
      "trigger_event": "Interaction(address,string,string[],bytes)",
      "trigger_json_path": "prediction_market.market_maker",
      "submit_json_path": "wavs_indexer"
    }
  ],
  "aggregator_components": [
    {
      "filename": "en0va_aggregator.wasm",
      "package_name": "en0va-aggregator",
      "package_version": "0.1.0",
      "env_variables": ["WAVS_ENV_ETHERSCAN_API_KEY"]
    }
  ]
}
