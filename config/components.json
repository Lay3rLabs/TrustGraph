      "filename": "merkler_pruner.wasm",
      "package_name": "merkler-pruner",
      "package_version": "0.7.0",
      "trigger": {
        "block_interval": {
          "blocks": 2000
        }
      },
      "submit": {
        "contract_json_path": "${networks[]}.contracts.merkleSnapshot"
      },
      "env_variables": []
    },
    {
      "filename": "wavs_safe_signer_sync.wasm",
