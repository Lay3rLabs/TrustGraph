{
  "name": "@wavs/trust-graph-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prebuild": "npm run indexer:config && npm run config:link && npm run wagmi:generate",
    "build": "next build",
    "predev": "npm run ensure-env-local && npm run indexer:config && npm run config:generate && npm run config:link && npm run wagmi:generate",
    "ensure-env-local": "if ! test -f .env.local; then cp .env.example .env.local; fi",
    "dev": "next dev",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "start": "next start",
    "indexer:config": "cp ../indexer/*.schema.ts .",
    "config:generate": "tsx scripts/generate-config.ts",
    "config:link": "rm -f config.json || true && test -f config.${NODE_ENV:-development}.json && ln -s config.${NODE_ENV:-development}.json config.json || echo \"Error: config.${NODE_ENV:-development}.json does not exist\"",
    "claude-code": "docker compose -f .claude/docker-compose.claude.yml run --rm --remove-orphans -it claude-code-sandbox bash -c 'claude; exec'",
    "claude-code:unrestricted": "docker compose -f .claude/docker-compose.claude.yml run --rm --remove-orphans -it claude-code-sandbox bash -c 'claude --dangerously-skip-permissions; exec'",
    "wagmi:generate": "wagmi generate"
  },
